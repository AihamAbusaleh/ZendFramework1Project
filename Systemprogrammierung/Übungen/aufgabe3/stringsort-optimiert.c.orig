#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<time.h>

/*int intcmp(const void *p, const void *q);
*/
int main(int argc, char *argv[])
{
	int i,j,n,m;
	char *s;
	time_t t; 
	srand((unsigned) time(&t));

	if(argc < 2)
	{
		printf("Aufruf: nicht genuegend Parameter ");
		return EXIT_FAILURE;
	}
	n = atoi(argv[1]);
	m = strlen(argv[1]);
	printf("\nunsortiertes Feld: \n");
	
	
	s =  (char *) malloc(((m*n) + 1) * sizeof(char));

	for(i = 0; i < n ; ++i ) 
	{	
		sprintf(s[i],"%d", rand() % n);
		printf("%s\n", s[i]);
	}
	
	printf("\nSortiertes Feld: \n");

	/*was mallociert, feldgroesse, size, sortierfunktion*/
/*	qsort(m,n, sizeof(char), intcmp);
*/
	for (i = n ; i > 1; --i){
		
		for(j = 0; j < i - 1 ; i++){
			if(strcmp(s[j], s[j+1]) > 0)
			{
				char* tmp = s[j + 1];
				s[j + 1] = s[j];  
				memcpy(s[j], tmp ,m);
			}
		}
	}

	printf("\n");

/*	int intcmp(const void *p, const void *q){
		const int *left = (const int *) p;
		const int *right = (const int *) q;
		if (*left > *right) return 1;
		if (*left < *right) return -1;
		return 0;
	
	}
*/
	free(s);
	return EXIT_SUCCESS;
}
