package blatt8;

public class Widerstand implements Schaltung { // entspricht dem Blatt in Design
	private int widerstand; // pattern

	public Widerstand(int widerstand) {
		this.widerstand = widerstand;
	}

	@Override
	public double getWiderstand() {

		return widerstand;
	}

	@Override
	public int getAnzahlWiderstaende() {
		int anz = 0;
		for (int i = 0; i < anz; i++) {
			anz += i.getWiderstand();
		return anz;
		}
	}

	@Override
	public void add(Schaltung s) {
		// bleibt leer

	}

	@Override
	public void remove(Schaltung s) {
		// bleibt leer

	}

}
