package blatt8;

import java.util.LinkedList;
import java.util.List;

public abstract class ZusammengesetzeSchaltung implements Schaltung{
	protected List<Schaltung> schaltung;

	
	public ZusammengesetzeSchaltung(List<Schaltung> schaltung) {
		List<Schaltung> schaltung = new LinkedList<Schaltung>();
	
	}

	public int getAnzahlWiderstaende() {
		int anz = 0;
		for (Schaltung s : schaltung) {
			anz += s.getWiderstand();
		}
		return anz;

	}

	public void add(Schaltung s) {
		schaltung.add(s);

	}

	public void remove(Schaltung s) {
		schaltung.remove(s);

	}
}
