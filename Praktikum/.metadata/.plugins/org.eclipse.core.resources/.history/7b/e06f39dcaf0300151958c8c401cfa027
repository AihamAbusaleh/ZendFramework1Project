package teil2;

public class Buch {

	private String name;
	private Benutzer user;

	public Buch(String name) {
		this.name = name;
	}

	public Benutzer getUser() {
		return user;
	}

	public void setUser(Benutzer u) {
		if(user != null)
			return; // user registriert !
		user = u;
		u.ausleihen(this);
	}

}
