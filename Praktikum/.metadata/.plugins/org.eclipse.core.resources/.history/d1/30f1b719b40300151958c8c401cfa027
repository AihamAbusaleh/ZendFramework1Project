package teil2;

public class TestBiblio {
	
	public static void main(String[] args) {
		Benutzer user1 = new Benutzer("ICH");
		user1.ausleihen(new Buch("Einführung in GL"));
		Buch buch = new Buch("Prog2");
		user1.ausleihen(buch);
		System.out.println(buch.getName() + " wurde jetzt ausgeliehen von : " + user1.getName()); 
		user1.zurueckgeben(buch);
		System.out.println("Das ausgeliehene Buch :" + buch.getName() + " wurde eben von " + user1.getName() + " zurückgegeben"); 

	}

}
