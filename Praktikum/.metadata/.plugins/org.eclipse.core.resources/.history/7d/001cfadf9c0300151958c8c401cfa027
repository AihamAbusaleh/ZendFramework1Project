package blatt8;

import java.util.List;

public abstract class ZusammengesetzeSchaltung {
	protected List<Schaltung> schaltung;
	
	public int getAnzahlWiderstaende() {
		int i = 0;
		for (Schaltung s : schaltung) {
			i += s.getWiderstand();
		}
		return i;

	}

	public void add(Schaltung s) {
		schaltung.add(s);
		
	}
	
	public void remove(Schaltung s) {
		schaltung.remove(s);
		
	}
}
